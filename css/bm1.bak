}
.tr-center{
  text-align : center;
  vertical-align : middle;
}
.t2{
	width: 95%;
	border: 0px;
	border-spacing: 3;
	border-collapse:collapse;
	padding:0;
	}
.t3, .t4, .t5{
	border-spacing: 0;
	border-collapse:collapse;
	padding:2;
  font-weight : normal;
  font-size : 1rem;
 	}
.t3{
	width: 95%;
  border: 0px;
 	font-family: IBM Plex Sans;
	}

.t3-p td{
  padding-top : 7px;
}

.t4{
	width: 100%;
  border: 0px;
 	font-family: IBM Plex Sans JP;
	}

.t5{
  width: 100%;
  border: 1px #000000 solid;
  font-family: IBM Plex Sans;
  font-weight : 400;
  font-size : 1.2rem;
  }
.t5 td{
  padding : 10px 5px 5px;
  border: 1px #000000 solid;
 }
